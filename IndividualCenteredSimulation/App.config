<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <log4net configSource="Log4NET.config" />
  <!--The different settings used by the application-->
  <appSettings>
    <!-- 89 | 50 | 30 -->
    <add key="GridSizeX" value="110" />
    <!-- 55 | 30 | 20 -->
    <add key="GridSizeY" value="70" />
    <add key="CanvasSizeX" value="700" />
    <add key="CanvasSizeY" value="500" />
    <add key="BoxSize" value="15" />
    <add key="DelayMilliseconde" value="25" />
    <!-- 0 = Unlimited -->
    <add key="TicksNumber" value="0" />
    <!-- True | False -->
    <add key="IsDisplayGrid" value="False" />
    <!-- True | False -->
    <add key="IsTraced" value="False" />
    <!-- True | False -->
    <add key="IsTracedPerformance" value="True" />
    <!-- 0 = Random -->
    <add key="Seed" value="0" />
    <add key="RateRefresh" value="1" />
    <!-- Fair | Sequential | Random -->
    <add key="SchedulingStrategy" value="Fair" />
    <add key="AgentsNumber" value="2000" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="SharpDX" publicKeyToken="b4dcf0f35e5521f1" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.1.1.0" newVersion="3.1.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="SharpDX.DXGI" publicKeyToken="b4dcf0f35e5521f1" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.1.1.0" newVersion="3.1.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="SharpDX.Direct2D1" publicKeyToken="b4dcf0f35e5521f1" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.1.1.0" newVersion="3.1.1.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>